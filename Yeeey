#include <stdio.h>
#include <stdlib.h>

int main()
{
   int g,aux,k,z,a,b,tab[50][50],i,j,randB,colB,randA,colA,A[50][50],B[50][50],C[50][50];
printf("Randuri= ");
scanf("%d", &randA);
printf("Coloane= ");
scanf("%d", &colA);
  for(i = 0; i < randA;i++){
 for(j = 0; j < colA; j++){
 printf("A[%d][%d]=", i,j);
 scanf("%d", &A[i][j]);
 }
 }
 printf("Randuri= ");
scanf("%d", &randB);
printf("Coloane= ");
scanf("%d", &colB);
 for(i = 0; i < randB;i++){
 for(j = 0; j < colB; j++){
 printf("B[%d][%d]=", i,j);
 scanf("%d", &B[i][j]);
 }
 }
 for(a=0,b=0;a<randA,b<colB;a++,b++)for(i = 0,g=0,k=0,j=0;g<randA&&g<colB,k<randB, j < colA;k++,j++){aux[g]+=A[i][j]*
 B[k][j];}

    return 0;
}
