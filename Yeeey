#include <stdio.h>
#include <stdlib.h>

int main()
{
int max,k,i,j,rand,col,tab[50][50],cab[50];
printf("Randuri= ");
scanf("%d", &rand);
printf("Coloane= ");
scanf("%d", &col);
max=rand*col;
  for(i = 0; i < rand;i++){
 for(j = 0; j < col; j++){
 printf("tablou[%d][%d]=", i,j);
 scanf("%d", &tab[i][j]);
 }
 }

 for(i = 0; i < rand;i++){
 for(j = 0; j < col; j++){for(k=0;k<max;k++){cab[k]=tab[i][j];
 printf("%2d", cab[k]);
 break;}}}


    return 0;
}
