#include <stdio.h>
#include <stdlib.h>

int main()
{
int nr_randuri,nr_coloane;
printf("Randuri");
scanf("%d", &nr_randuri);
printf("Coloane");
scanf("%d",&nr_coloane);
int suma;
int i,j;
int tab[50][50];
 for(i = 0; i < nr_randuri;i++){
 for(j = 0; j < nr_coloane; j++){
 printf("tablou[%d][%d]=", i,j);
 scanf("%d", &tab[i][j]);
 }
 }
 for(i = 0; i < nr_randuri;i++){
 for(j = 0; j < nr_coloane; j++){ suma+=tab[i][j];}}
 printf("Suma elementelor= %d", suma);

    return 0;
}
