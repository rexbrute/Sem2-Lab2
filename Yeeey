#include <stdio.h>
#include <stdlib.h>

int main()
{
  int i,j,rand,col,A[50][50],B[50][50],C[50][50];
printf("Randuri= ");
scanf("%d", &rand);
printf("Coloane= ");
scanf("%d", &col);
  for(i = 0; i < rand;i++){
 for(j = 0; j < col; j++){
 printf("A[%d][%d]=", i,j);
 scanf("%d", &A[i][j]);
 }
 }
 for(i = 0; i < rand;i++){
 for(j = 0; j < col; j++){
 printf("B[%d][%d]=", i,j);
 scanf("%d", &B[i][j]);
 }
 }
 printf("Suma celor doua matrici");
 printf("\n");
 for(i = 0; i < rand;i++){
 for(j = 0; j < col; j++){
 C[i][j]=A[i][j]+B[i][j];
 printf("%2d", C[i][j]);
 }
 }
    return 0;
}

