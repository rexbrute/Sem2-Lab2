#include <stdio.h>
#include <stdlib.h>

int main()
{int suma,rand,i,j,A[50][50],tab[50][50];
 printf("Randuri= ");
scanf("%d", &rand);
int col=rand;
  for(i = 0; i < rand;i++){
 for(j = 0; j < col; j++){
 printf("A[%d][%d]=", i,j);
 scanf("%d", &A[i][j]);}}
 for(i = 0, j = 0;i < rand,j<rand;j++,i++){
 tab[i][j]=A[i][j];
 printf("%2d", A[i][j]);}
 for(i = 0; i < rand;i++){
 for(j = 0; j < col; j++){suma+=tab[i][j];}}
 printf("\nSuma elementelor de pe diagonala este = %d", suma);

    return 0;
}

