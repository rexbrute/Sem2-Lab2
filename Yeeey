#include <stdio.h>
#include <stdlib.h>

int afiseaza(int tab[], int n)
{
    int i = 0;
    for(i = 0; i< n; i++)
    {
        printf("%d ", tab[i]);
    }

}

int main()
{
    int tab[] = {1,2,3,4};
    int m = 2;
    int n = 4;
    int matrice[][4] = {{1,2,3,4},
                        {5,6,7,8}};

    afiseaza(tab, n);
    printf("\n");
    afiseaza(matrice, m*n);
    return 0;
}

